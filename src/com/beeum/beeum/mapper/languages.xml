<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="languages">
	<select id="selectOne"
	parameterType="String"
	resultType="int"
	>
		SELECT no_lang
		FROM languages
		WHERE language = #{language}
	</select>
</mapper>  










