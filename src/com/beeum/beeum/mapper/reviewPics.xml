<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="reviewPics">
	
	<select id="selectListTlr" parameterType="int" resultType="String">
		select file_name
		from review_pics
		where no_tlr = #{no}
	</select>
	
	<select id="selectListTtr" parameterType="int" resultType="String">
		select file_name
		from review_pics
		where no_ttr = #{no}
	</select>
	
</mapper>  










